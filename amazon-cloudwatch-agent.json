{
	"agent": {
		"run_as_user": "root"
	},
	"logs": {
		"logs_collected": {
			"files": {
				"collect_list": [
					{
						"file_path": "/var/log/syslog",
						"log_group_name": "syslog",
						"log_stream_name": "{instance_id}"
					},
					{
						"file_path": "/var/log/auth.log",
						"log_group_name": "auth.log",
						"log_stream_name": "{instance_id}"
					},
					{
						"file_path": "/var/log/kern.log",
						"log_group_name": "kern.log",
						"log_stream_name": "{instance_id}"
					},
					{
						"file_path": "/log/monitoring.log",
						"log_group_name": "monitoring.log",
						"log_stream_name": "{instance_id}"
					},
					{
						"file_path": "/log/app.log",
						"log_group_name": "app.log",
						"log_stream_name": "{instance_id}"
					},
					{
						"file_path": "/log/debug.log",
						"log_group_name": "debug.log",
						"log_stream_name": "{instance_id}"
					},
					{
						"file_path": "/log/request.log",
						"log_group_name": "request.log",
						"log_stream_name": "{instance_id}"
					},
					{
						"file_path": "/log/requestjson.log",
						"log_group_name": "requestjson.log",
						"log_stream_name": "{instance_id}"
					}
				]
			}
		}
	}
